<h1>Выносы</h1>
<p id="notice"><%= notice %></p>
<table>
  <tr>
    <th>Имя выноса</th>
    <th>Примечание</th>
    <th>Точка терминирования</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @vinos.each do |vino| %>
  <tr>
    <td><%= vino.vinosname %></td>
    <td><%= vino.descr %></td>
    <td><%= vino.terminate_point.routerip %></td>
    <td><%= link_to 'Редактировать', edit_vino_path(vino) %></td>
    <td><%= link_to 'Удалить', vino, confirm: 'Вы уверены?', method: :delete %></td>
    <td><%= link_to 'Вланы этого выноса', "vinovlans/#{vino.id}", method: :post %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Новый вынос', new_vino_path %>
