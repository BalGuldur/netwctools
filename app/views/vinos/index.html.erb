  </tr>

<% @vinos.each do |vino| %>
  <tr>
    <td><%= vino.vinosname %></td>
    <td><%= vino.descr %></td>
    <td><%= vino.terminate_point.routerip %></td>
    <td><%= link_to 'Show', vino %></td>
    <td><%= link_to 'Edit', edit_vino_path(vino) %></td>
    <td><%= link_to 'Destroy', vino, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

